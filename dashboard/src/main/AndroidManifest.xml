<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.avisio.dashboard">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.Avisio">
        <activity android:name=".activity.edit_box.EditBoxActivity"
                  android:label="@string/activity_edit_box"
                  android:theme="@style/Theme.Avisio.NoActionBar">
        </activity>
        <activity android:name=".activity.box_activity.BoxActivity">
        </activity>
        <activity
                android:name=".activity.create_box.CreateBoxActivity"
                android:label="@string/title_activity_create_box"
                android:theme="@style/Theme.Avisio.NoActionBar">
        </activity>
        <activity
                android:name=".activity.box_list.MainActivity"
                android:label="@string/app_name"
                android:theme="@style/Theme.Avisio.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>